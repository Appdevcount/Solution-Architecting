   "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test":"react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --ci --coverage --collectCoverageFrom=src/**/*.{ts,tsx} --testResultsProcessor=jest-sonar-reporter --reporters=jest-junit --coverageReporters=cobertura --coverageReporters=lcov --coverageReporters=html --watchAll=false",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
  },
 
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/mocks/**",
      "!**/common/**"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
